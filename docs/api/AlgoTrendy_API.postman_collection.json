				{
					"name": "Get Market Data",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{baseUrl}}/api/{{apiVersion}}/marketdata?symbol=BTCUSDT&exchange=binance&interval=1h&limit=100",
							"host": ["{{baseUrl}}"],
							"path": ["api", "{{apiVersion}}", "marketdata"],
							"query": [
								{
									"key": "symbol",
									"value": "BTCUSDT",
									"description": "Trading symbol (e.g., BTCUSDT, ETHUSDT, AAPL)"
								},
								{
									"key": "exchange",
									"value": "binance",
									"description": "Exchange name (binance, bybit, coinbase, etc.)"
								},
								{
									"key": "interval",
									"value": "1h",
									"description": "Time interval (1m, 5m, 15m, 1h, 4h, 1d)"
								},
								{
									"key": "limit",
									"value": "100",
									"description": "Number of candles to retrieve"
								}
							]
						},
						"description": "Retrieve historical market data (OHLCV candles) for a symbol"
					},
					"response": []
				},
