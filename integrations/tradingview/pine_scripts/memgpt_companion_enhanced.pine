        table.cell(mem_table, 0, 10, "â° Timeframe", text_color=color.black)
        table.cell(mem_table, 1, 10, str.upper(str.replace_all(mem_timeframe, " ", "_")), text_color=color.black)
        
        table.cell(mem_table, 0, 11, "ğŸ”— Server", text_color=color.black)
