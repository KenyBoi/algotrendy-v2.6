// === MAIN SETTINGS ===
server_ip = input.string("216.238.90.131", title="ğŸŒ MEM Server IP")
server_port = input.int(5003, title="ğŸ”Œ Server Port")
update_freq = input.int(5, title="â±ï¸ Update Frequency (sec)", minval=3, maxval=60)
