{
  "webhook_url": "http://YOUR_SERVER:5004/webhook",
  "alert_templates": {
    "basic_buy_sell": {
      "name": "MemGPT Basic Buy/Sell Alert",
      "message": {
        "symbol": "{{ticker}}",
        "action": "{{strategy.market_position}}",
        "price": "{{close}}",
        "timestamp": "{{time}}",
        "volume": "{{volume}}",
        "source": "tradingview"
      },
      "description": "Basic buy/sell alert for MemGPT analysis"
    },
    
    "enhanced_signal": {
      "name": "MemGPT Enhanced Trading Signal",
      "message": {
        "symbol": "{{ticker}}",
        "action": "{{strategy.market_position}}",
        "price": "{{close}}",
        "high": "{{high}}",
        "low": "{{low}}",
        "volume": "{{volume}}",
        "rsi": "{{rsi}}",
        "macd": "{{macd}}",
        "timestamp": "{{time}}",
        "timeframe": "{{interval}}",
        "source": "tradingview_enhanced"
      },
      "description": "Enhanced signal with technical indicators"
    },
    
    "multi_timeframe": {
      "name": "MemGPT Multi-Timeframe Signal",
      "message": {
        "symbol": "{{ticker}}",
        "action": "{{strategy.market_position}}",
        "price": "{{close}}",
        "timeframe": "{{interval}}",
        "direction": "{{strategy.direction}}",
        "confidence": "{{strategy.confidence}}",
        "timestamp": "{{time}}",
        "source": "tradingview_mtf"
      },
      "description": "Multi-timeframe analysis signal"
    }
  },
  
  "setup_instructions": {
    "step1": "Copy the webhook_url to your TradingView alert configuration",
    "step2": "Select appropriate alert template based on your needs",
    "step3": "Paste the message JSON into TradingView alert message field",
    "step4": "Configure alert conditions in your Pine Script",
    "step5": "Test with a small position size first"
  },
  
  "webhook_testing": {
    "test_url": "http://localhost:5004/webhook",
    "test_message": {
      "symbol": "BTCUSDT",
      "action": "BUY",
      "price": 50000,
      "timestamp": "2025-10-15T00:00:00Z",
      "source": "test"
    },
    "curl_command": "curl -X POST -H 'Content-Type: application/json' -d '{\"symbol\":\"BTCUSDT\",\"action\":\"BUY\",\"price\":50000}' http://localhost:5004/webhook"
  }
}